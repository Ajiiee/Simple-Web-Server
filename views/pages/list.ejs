<!DOCTYPE html>
<html lang="en">
<%- include('../partials/header'); %>

<body class="text-center">
  <%- include('../partials/menu') %>

  <section name="jumbotron">
    <div class="jumbotron text-center">
      <!-- <h1>Form Ninja</h1>
      <p>Build With EJS</p> -->
    </div>
  </section>

  <h1>Person List</h1>

  <% for( let i = 0; i < person.length; i++){ %>
  <li>
    <input type="checkbox" name="check" value="<%= person[i] %>" /> Name : <%=
          person[i].name %>,</br> Email : <%= person[i].email %>, <br> Class : <%= person[i].class%>, </br>
    School : <%= person[i].school %>,<br> Alamat : <%= person[i].alamat %>
  </li>
  <% } %>

  <%- include('../partials/footer') %>
  <%- include('../partials/script') %>
</body>

</html>